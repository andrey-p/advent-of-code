{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "types": ["node"],
    "noEmit": true,

    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/",
      "node_modules/@types"
    ]
  },
  "include": ["solutions", "runner.ts"]
}
